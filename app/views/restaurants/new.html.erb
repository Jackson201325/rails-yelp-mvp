<div class="container">
  <h1>Create Restaurant</h1>
  <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category %>
    <%= f.button :submit, class:'btn btn-success'%>
    <%= link_to 'Restaurants', restaurants_path , class: 'btn btn-primary' %>
  <% end %>
</div>